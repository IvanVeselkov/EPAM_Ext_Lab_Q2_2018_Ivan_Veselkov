@model TestingKnowLedgeVIS.Models.IndexViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <td>TestId</td>
        <td>Test</td>
    </tr>
    @foreach (var item in Model.Tests)
    {
        <tr>
            <td>@item.TestId</td>
            <td>@item.TestName</td>
        </tr>
    }
</table>
<br />
