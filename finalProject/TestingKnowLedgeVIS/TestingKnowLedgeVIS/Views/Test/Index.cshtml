@model TestingKnowLedgeVIS.Models.IndexViewModel
@using TestingKnowLedgeVIS.Models

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <td>TestId</td>
        <td>Test</td>
    </tr>
    @foreach (var item in Model.Tests)
    {
        <tr>
            <td>@item.ID</td>
            <td>@Html.ActionLink(item.Description, "PreStartTest", new { testid = item.ID})</td>
        </tr>
    }
</table>
<br />
